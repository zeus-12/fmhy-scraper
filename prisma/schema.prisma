generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Wiki {
  id    String  @id @default(cuid())
  title String  @db.LongText
  // starred Boolean @default(false)
  link  String  @db.LongText
  nsfw  Boolean @default(false)
}

model Base64 {
  id    String @id @default(cuid())
  title String @db.LongText
  hash  String @db.LongText
}

model Guides {
  id      String  @id @default(cuid())
  credits String
  link    String
  nsfw    Boolean @default(false)
  tags    String  @default("[]")
  title   String  @db.LongText
}
